<h1>Show User</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @user.name %>
  </li>

  <li>
    <strong>Username:</strong>
    <%= @user.username %>
  </li>

  <li>
    <strong>Provider:</strong>
    <%= @user.fed_credential.provider %>
  </li>

  <li>
    <strong>Token:</strong>
    <%= @user.fed_credential.token %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.users_user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: Routes.users_user_path(@conn, :index) %></span>
